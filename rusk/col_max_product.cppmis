

int maximum_pairwise(const vector<int>& a)
{
	int max1 = 0, max2 = 0;
	for(int i = 1; i < a.size(); i++){
		if(max1 < a[i]){
			max1 = i
		}
	}

	if(max1 == 0)
		max2 = 1


	for(int i = 1; i < a.size(); i++){
		if(max2 < a[i] && a[i] != a[max1]){
			max2 = i
		}
	}

	return max1 * max2
}







if( a[0] >= a[1]){
	max1 = a[0];
	max2 = a[1];
}
else{
	max1 = a[1];
	max2 = a[0];
}




